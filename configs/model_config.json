{
  "model_name": "CatBoy-v2",
  "architecture": "decoder-only-transformer",

  "vocab_size": 8000,
  "block_size": 128,

  "n_layers": 4,
  "n_heads": 4,
  "embedding_dim": 256,
  "ffn_multiplier": 4,

  "dropout": 0.1,
  "attention_dropout": 0.1,

  "use_rope": true,
  "use_rmsnorm": true,
  "tie_embeddings": true,

  "lr": 0.0003,
  "batch_size": 8,
  "epochs": 5,

  "warmup_steps": 100,
  "max_steps": 2000,

  "min_token_freq": 2,

  "max_new_tokens": 128,
  "temperature": 0.9,
  "top_k": 50,
  "top_p": 0.95,
  "repetition_penalty": 1.1
}
